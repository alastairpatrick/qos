{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug RTOS",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/build/qos.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "device": "Pico2040",
            "runToMain": true,
            "configFiles": ["interface/picoprobe.cfg", "target/rp2040.cfg"],
            "searchDir": ["${env:OPENOCD_PATH}/tcl"],
            "svdFile": "${workspaceRoot}/pico-sdk/src/rp2040/hardware_regs/rp2040.svd"
        }
    ]
}
